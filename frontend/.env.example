VITE_API_URL="https://your-backend.example.com"
VITE_SUPABASE_PROJECT_ID="your-supabase-project-id"
# This should be the publishable (anon) key if your frontend needs direct Supabase auth.
# Avoid committing the real key. Use your environment/service provider secrets management.
VITE_SUPABASE_PUBLISHABLE_KEY="your-publishable-key"
VITE_SUPABASE_URL="https://your-project.supabase.co"

# If you want the frontend to talk only to your backend (recommended), set VITE_API_URL and
# leave the Supabase client keys empty so the app uses the backend proxy endpoints.
